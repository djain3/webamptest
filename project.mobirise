          "_name": "features1",
          "_customHTML": "<section class=\"features1\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n    <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <select title=\"Block Type\" name=\"blockType\">\n        <option value=\"1\">Horizontal Cards With Only Text</option>\n        <option value=\"2\">Vertical Cards With Only Text</option>\n        <option value=\"3\">Horizontal Cards With Icons</option>\n        <option value=\"4\">Vertical Cards With Icons</option>\n        <option value=\"5\">Horizontal Cards With Counters</option>\n        <option value=\"6\">Vertical Cards With Counters</option>\n        <option value=\"7\">Horizontal Cards With Images 1</option>\n        <option value=\"8\" selected=\"\">Vertical Cards With Images</option>\n        <option value=\"9\">Horizontal Cards With Images 2</option>\n    </select>\n    <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked=\"\">\n    <input type=\"range\" inline=\"\" title=\"Title Block Width\" name=\"textWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"100\" condition=\"showTitle || showSubtitle || showText || showButtons\">\n    <select title=\"Title Block Align\" name=\"textAlign\" condition=\"showTitle || showSubtitle || showText || showButtons\">\n        <option value=\"flex-start\">Left</option>\n        <option value=\"center\" selected=\"\">Center</option>\n        <option value=\"flex-end\">Right</option>\n    </select>\n    <input type=\"range\" inline=\"\" title=\"Cards Count\" name=\"cardsCount\" min=\"1\" max=\"8\" step=\"1\" value=\"3\">\n    <input type=\"range\" inline=\"\" title=\"Cards Width\" name=\"cardsWidth\" min=\"20\" max=\"100\" step=\"1\" value=\"60\" condition=\"blockType == 1 || blockType == 3 || blockType == 5 || blockType == 7 || blockType == 9\">\n    <select title=\"Cards Align\" name=\"cardsAlign\" condition=\"blockType == 1 || blockType == 3 || blockType == 5 || blockType == 7 || blockType == 9\">\n        <option value=\"flex-start\">Left</option>\n        <option value=\"center\" selected=\"\">Center</option>\n        <option value=\"flex-end\">Right</option>\n    </select>\n    <input type=\"checkbox\" title=\"Show Cards Title\" name=\"showCardsTitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Cards Subtitle\" name=\"showCardsSubtitle\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Cards Text\" name=\"showCardsText\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Cards Buttons\" name=\"showCardsButtons\" checked=\"\">\n    <input type=\"checkbox\" title=\"Show Borders\" name=\"showBorders\">\n    <input type=\"color\" title=\"Borders Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders\">\n    <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\">\n    <input type=\"color\" title=\"Card BG Color\" name=\"cardBg\" value=\"#ffffff\" condition=\"!transparentBg\">\n    <input type=\"range\" inline=\"\" title=\"Sizes\" name=\"sizes\" min=\"10\" max=\"90\" step=\"1\" value=\"20\" condition=\"blockType == 3 || blockType == 5 || blockType == 7 || blockType == 9\">\n    <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"blockType == 7 || blockType == 9\">\n    <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#66458e\" condition=\"blockType == 3 || blockType == 4\">\n    <input type=\"range\" inline=\"\" title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"8\" step=\"0.5\" value=\"4\" condition=\"blockType == 3 || blockTyp