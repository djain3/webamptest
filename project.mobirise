          "_customHTML": "<section class=\"team1\" group=\"Teams\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n\n        <select title=\"Block Type\" name=\"blockType\">\n            <option value=\"1\" selected=\"\">Standart Cards 1</option>\n            <option value=\"2\">Standart Cards 2</option>\n            <option value=\"3\">Cards with Body on Image 1</option>\n            <option value=\"4\">Cards with Body on Image 2</option>\n            <option value=\"5\">Cards with Icons on Image 1</option>\n            <option value=\"6\">Cards with Icons on Image 2</option>\n            <option value=\"7\">Cards with only Text 1</option>\n            <option value=\"8\">Cards with only Text 2</option>\n            <option value=\"9\">Horizontal Cards with Images 1</option>\n            <option value=\"10\">Horizontal Cards with Images 2</option>\n        </select>\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"range\" inline=\"\" title=\"Cards Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n        <input type=\"checkbox\" title=\"Rounded Image\" name=\"roundedImage\" condition=\"blockType == 1 || blockType == 2\">\n        <input type=\"range\" inline=\"\" title=\"Image Width\" name=\"imageWidth\" min=\"50\" max=\"100\" step=\"1\" value=\"100\" condition=\"blockType == 1 || blockType == 2\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imageOverlayColor\" value=\"#ffffff\" condition=\"blockType > 2 &amp;&amp; blockType < 7\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imageOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"blockType > 2 &amp;&amp; blockType < 7\">\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showName\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Specialty\" name=\"showSpecialty\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Icons Count\" name=\"iconsCount\" min=\"0\" max=\"4\" step=\"1\" value=\"3\" condition=\"blockType != 7 &amp;&amp; blockType != 8\">\n        <input type=\"range\" inline=\"\" title=\"Icons Size\" name=\"iconsSize\" min=\".5\" max=\"2\" step=\"0.1\" value=\"1.3\" condition=\"blockType != 7 &amp;&amp; blockType != 8\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#66458e\" condition=\"blockType != 7 &amp;&amp; blockType != 8\">\n        <input type=\"color\" title=\"Icons Hover BG Color\" name=\"iconsBg\" value=\"#66458e\" condition=\"blockType == 5 || blockType == 6\">\n        <input type=\"color\" title=\"Icons Hover Color\" name=\"iconsHover\" value=\"#fac769\" condition=\"blockType == 5 || blockType == 6\">\n        <input type=\"checkbox\" title=\"Image on Left/Right\" name=\"reverseContent\" condition=\"blockType > 8\">\n        <input type=\"checkbox\" title=\"Transparent Card BG\" name=\"transparentBg\" condition=\"blockType != 3 &amp;&amp; blockType != 4\">\n        <input type=\"color\" title=\"Card BG\" name=\"cardBg\" value=\"#ffffff\" condition=\"!transparentBg &amp;&amp; blockType != 3 &amp;&amp; blockType != 4\">\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\" selected=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n        </fieldset>\n        \n        <input type=\"checkbox\" title=\"Overlay\" nam