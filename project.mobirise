          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".gallery-img-wrap": {
              "height": "auto",
              "&:hover": {
                "&:after": {
                  "opacity": "@imgOverlayOpacity"
                }
              },
              "&:after": {
                "background": "@imgOverlayColor"
              },
              "span": {
                "font-size": "~\"@{imgIconsSize}rem\"",
                "width": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
                "height": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
                "background-color": "@imgIconsBgColor",
                "color": "@imgIconsColor"
              }
            },
            "amp-img,img": {
              "height": "auto",
              "width": "100%",
              "object-fit": "contain"
            },
            ".gallery-row": {
              "@media (max-width: 992px)": {
                "justify-content": "center"
              },
              "padding-left": "0",
              "padding-right": "0"
            },
            ".gallery-item, .gallery-row": {
              "&:nth-last-child(1)": {
                "padding-bottom": "0"
              }
            },
            "& when not (@paddingCard)": {
              ".gallery-item, .gallery-row": {
                "padding": "0!important"
              }
            },
            "& when (@siteStyle = Photography)": {
              ".containers-wrap": {
                "justify-content": "flex-start",
                ".items-row": {
                  "justify-content": "flex-start",
                  "@media (max-width: 992px)": {
                    "justify-content": "center"
                  }
                }
              }
            },
            "& when (@siteStyle = Portfolio)": {
              ".gallery-img-wrap:after": {
                "left": "5px",
                "top": "5px",
                "right": "5px",
                "bottom": "5px"
              }
            }
